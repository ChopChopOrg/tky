{
  "name": "tky",
  "version": "0.1.3",
  "main": "dist/index.js",
  "umd:main": "dist/tky.umd.production.js",
  "module": "dist/tky.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "commit": "git-cz",
    "release": "yarn build && standard-version && git push && yarn publish"
  },
  "peerDependencies": {
    "ky": "^0.19.0"
  },
  "dependencies": {
    "utility-types": "^3.10.0"
  },
  "license": "MIT",
  "bugs": "https://github.com/ChopChopOrg/tky/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChopChopOrg/tky.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.2.1",
    "cz-conventional-changelog": "3.1.0",
    "husky": "^4.2.3",
    "ky": "^0.19.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "standard-version": "^8.0.1",
    "ts-snippet": "^4.2.0",
    "tsdx": "^0.13.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
